<h1>Publications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shared_publications.each do |shared_publication| %>
      <tr>
        <%= render 'publications/publication', publication: shared_publication.publication %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publication', new_publication_path %>
